<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Project_Info_Item">
        <!--左边图片资源-->
        <attr name="left_image" format="reference|integer"/>
        <!--标题框文字-->
        <attr name="title_text" format="string"/>
        <!--内容框文字-->
        <attr name="content_text" format="string"/>
        <!--右边箭头资源-->
        <attr name="right_arrow" format="reference|integer"/>
        <!--控件背景-->
        <attr name="item_background_color" format="color"/>
        <!--标题框背景-->
        <attr name="title_text_background" format="reference|integer"/>
        <!--内容框背景-->
        <attr name="content_text_background" format="reference|integer"/>
        <!--右边箭头是否显示-->
        <attr name="right_arrow_visible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float"/>
    </declare-styleable>

    <declare-styleable name="NineGridLayout">
        <attr name="sapcing" format="dimension"/>
        <attr name="oneImageWidth" format="dimension"/>
        <attr name="oneImageHeight" format="dimension"/>
        <attr name="image_ratio" format="float"/>
    </declare-styleable>
</resources>